{
  "name": "electron-boilerplate",
  "version": "1.0.0",
  "description": "Scaffold out an Electron application",
  "main": "./lib/launch.js",
  "scripts": {
    "dev": "APP_ENV=dev electron .",
    "start": "electron .",
    "lint:js": "eslint ./lib",
    "build:osx": "electron-packager ./ 'electron-boilerplate' --app-version=1.0.0 --out=./build --ignore=./build --platform=darwin --arch=x64 --icon=assets/osx/icon.icns --overwrite --prune"
  },
  "repository": "https://github.com/xudafeng/electron-boilerplate",
  "keywords": [
    "Electron",
    "demo"
  ],
  "license": "MIT",
  "devDependencies": {
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "electron-packager": "^14.1.1",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.0",
    "stringify": "^5.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "vue": "^2.1.10"
  }
}
